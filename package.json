  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testMatch='**/__tests__/unit/**/*.test.{ts,tsx}'",
    "test:integration": "jest --testMatch='**/__tests__/integration/**/*.test.{ts,tsx}'",
    "test:e2e": "playwright test",
    "test:e2e:bypass": "cross-env E2E_TESTING=true playwright test navigation-with-bypass.spec.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "echo 'Tests disponibles pero no ejecutados autom√°ticamente'",
